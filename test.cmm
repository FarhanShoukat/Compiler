## prints numbers between a and b
   returns max of a and b ##
int function(int a, int b) {
    int i, j, max;
    i := a + 1;
    j := b;

    out "Numbers between x and y:";
    while(i < j) {      #prints numbers between i (a) and j (b)
        out i;
    }

    if(a > b) {         #returns max of both
        max := a;
    }
    else {
        max := b;
    }

    ret max;
}

int x, y, m;

out "x:";
in x;

out "y:";
in y;

m := function(x, y);

out "max of x and y:";
out m;